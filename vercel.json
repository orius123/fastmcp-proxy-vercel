{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "env": {
    "DESCOPE_CONFIG_URL": "@descope-config-url",
    "TARGET_MCP_SERVER_URL": "@target-mcp-server-url",
    "TARGET_TOKEN": "@target-token"
  }
}
